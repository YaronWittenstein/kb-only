{
  "name": "kb-only",
  "version": "1.0.0",
  "description": "vscode extension to ease using only the keyboard",
  "publisher": "YaronWittenstein",
  "engines": {
    "vscode": "^1.53.0"
  },
  "activationEvents": [
    "*"
  ],
  "main": "out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "kb-only.next",
        "title": "Jump to next Editor/Terminal"
      }
    ]
  },
  "scripts": {
    "test": "npm test",
    "compile": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YaronWittenstein/kb-only"
  },
  "keywords": [
    "productivity"
  ],
  "author": "Yaron Wittenstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/YaronWittenstein/kb-only/issues"
  },
  "homepage": "https://github.com/YaronWittenstein/kb-only#readme",
  "devDependencies": {
    "@types/node": "^14.14.25",
    "@types/vscode": "^1.53.0",
    "typescript": "^4.1.3"
  }
}
